<!-- pages/orderinfo/orderinfo.wxml -->
<view class="container">
  <view class="orderinfo_wrap">
    <!-- 搜索模块 -->
    <!-- <view class="orderinfo_top" hover-class="none" hover-stop-propagation="false">
      <view class="orderinfo_header" hover-class="none" hover-stop-propagation="false">
        <text>设备详情</text>
      </view>
    </view> -->
    <!-- 订单详情 -->
    <scroll-view class="info_main" scroll-y='true'>
      <view class="modle">
        <view class="order_info_modle">
          <label for="">
            <!-- <text class="iconfont icon-dian:before"></text> -->
            设备编号:
          </label>
          <text>{{deviceInfo.devNo}}</text>
        </view>
        <view class="order_info_modle">
          <label for="">
            <!-- <text class="iconfont icon-dian:before"></text> -->
             通讯编号:
          </label>
          <text>{{deviceInfo.machNo}}</text>
        </view>
        <view class="order_info_modle">
          <label for="">
            <!-- <text class="iconfont icon-dian:before"></text> -->
             咖啡机名称:
          </label>
          <text>{{deviceInfo.name}}</text>
        </view>
      </view>


      <view class="modle">
        <view class="order_info_modle">
          <label for="">
            <!-- <text class="iconfont icon-dian:before"></text> -->
            是否可用:
          </label>
          <text>{{deviceInfo.statusStr}}</text>
        </view>
        <view class="order_info_modle">
          <label for="">
            <!-- <text class="iconfont icon-dian:before"></text> -->
             咖啡机摆放地址:
          </label>
          <text>{{deviceInfo.address}}</text>
        </view>
        <view class="order_info_modle">
          <label for="">
            <!-- <text class="iconfont icon-dian:before"></text> -->
             第一杯重置天数:
          </label>
          <text>{{deviceInfo.cookieValid}}</text>
        </view>
      </view>

    
      <view class="modle">
        <view class="order_info_modle">
          <label for="">
            <!-- <text class="iconfont icon-dian:before"></text> -->
           第一种咖啡价格:
          </label>
          <text>{{deviceInfo.price1}}</text>
        </view>
        <view class="order_info_modle">
          <label for="" class="dis">
            <!-- <text class="iconfont icon-dian:before"></text> -->
             限制杯数:
          </label>
          <text>{{deviceInfo.price1Limit}}</text>
        </view>
      </view>

      <view class="modle">
        <view class="order_info_modle">
          <label for="">
            <!-- <text class="iconfont icon-dian:before"></text> -->
           第二种咖啡价格:
          </label>
          <text>{{deviceInfo.price2}}</text>
        </view>
        <view class="order_info_modle">
          <label for=""  class="dis">
            <!-- <text class="iconfont icon-dian:before"></text> -->
             限制杯数:
          </label>
          <text>{{deviceInfo.price2Limit}}</text>
        </view>
      </view>

      <view class="modle">
        <view class="order_info_modle">
          <label for="">
            <!-- <text class="iconfont icon-dian:before"></text> -->
           第三种咖啡价格:
          </label>
          <text>{{deviceInfo.price3}}</text>
        </view>
        <view class="order_info_modle">
          <label for=""  class="dis">
            <!-- <text class="iconfont icon-dian:before"></text> -->
             限制杯数:
          </label>
          <text>{{deviceInfo.price3Limit}}</text>
        </view>
      </view>

      <view class="modle">
        <view class="order_info_modle">
          <label for="">
            <!-- <text class="iconfont icon-dian:before"></text> -->
           所属客户:
          </label>
          <text>{{deviceInfo.customerName}}</text>
        </view>
        <view class="order_info_modle">
          <label for="">
            <!-- <text class="iconfont icon-dian:before"></text> -->
             所属城市:
          </label>
          <text>{{deviceInfo.customerAddress}}</text>
        </view>
      </view>

      <view class="modle">
        <view class="order_info_modle">
          <label for="">
            <!-- <text class="iconfont icon-dian:before"></text> -->
           备　　注:
          </label>
          <text>{{deviceInfo.remark}}</text>
        </view>
        <view class="order_info_modle">
          <label for="">
            <!-- <text class="iconfont icon-dian:before"></text> -->
             离线状态:
          </label>
          <text>{{deviceInfo.statusStr}}</text>
        </view>
        <view class="order_info_modle">
          <label for="">
            <!-- <text class="iconfont icon-dian:before"></text> -->
             使用杯数:
          </label>
          <text>{{deviceInfo.usesCount}}</text>
        </view>
      <navigator hover-class="none" url="../modify/modify?devNo={{deviceInfo.devNo}}">
        <button style="background: #2a8eff; color:#ffffff">修改设备</button>
      </navigator>
      </view>
     
    </scroll-view>
  </view>
</view>